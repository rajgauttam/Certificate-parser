<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="csm" targetNamespace="csm">

   <xs:include schemaLocation="message.xsd"/>

    <!-- Entry - Base class for all entries -->
    <xs:complexType name="Entry">
        <xs:annotation>
            <xs:documentation>Base class for all Entry objects</xs:documentation>
        </xs:annotation>
    </xs:complexType>

    <!-- NamedEntry - Represents text and literals -->
    <xs:element name="namedEntry" type="NamedEntry" />
    <xs:complexType name="NamedEntry">
        <xs:annotation>
            <xs:documentation>Represents named and literal entries</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="Entry">
                <xs:sequence>
                    <xs:element name="type" type="xs:string" minOccurs="1" maxOccurs="1" />
                    <xs:element name="name" type="xs:string" minOccurs="1" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation>Name of entry</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="literal" type="xs:boolean" minOccurs="1" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation>True if entry is literal value (name will represent value as well)</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="value" type="xs:string" minOccurs="0" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation>Optional value of entry, can be same as Name</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    

    <!-- ObjectEntry - Represents objects (have id) -->
    <xs:element name="objectEntry" type="ObjectEntry" />
    <xs:complexType name="ObjectEntry">
        <xs:annotation>
            <xs:documentation>Represents objects with id</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="NamedEntry">
                <xs:sequence>
                    <xs:element name="uuid" type="xs:string" minOccurs="0" maxOccurs="1"></xs:element>
                    <xs:element name="id" type="xs:unsignedLong" minOccurs="0" maxOccurs="1"></xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="Attributes">
        <xs:annotation>
            <xs:documentation>Can extend Attributes and add platform specific attributes</xs:documentation>
        </xs:annotation>
    </xs:complexType>

	<!-- Certificate Node Entry  -->
  <xs:element name="certificateNodeEntry" type="CertificateNodeEntry" />
  	<xs:complexType name="CertificateNodeEntry">
		<xs:annotation>
			<xs:documentation>Certificate Node Object Entry</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="Entry">
				<xs:all>
				<xs:element name="key" type="xs:string" minOccurs="0" maxOccurs="1" />
				<xs:element name="value" type="xs:string" minOccurs="0" maxOccurs="1" />
                <xs:element name="children" type="CertificateNodeEntry"
                         minOccurs="0" maxOccurs="unbounded" />
				</xs:all>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

    <!-- PolicyObjectEntry - Base class for all objects -->
    <xs:element name="policyObjectEntry" type="PolicyObjectEntry" />
    <xs:complexType name="PolicyObjectEntry">
        <xs:annotation>
            <xs:documentation>Representation of policy objects</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="ObjectEntry">
                <xs:sequence>
                    <xs:element name="subType" type="xs:string" minOccurs="0" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation>Object Sub-Type</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="toolTip" type="xs:string" minOccurs="0" maxOccurs="1" />
                    <xs:element name="description" type="xs:string" minOccurs="0" maxOccurs="1" />
                    <xs:element name="readOnly" type="xs:boolean" minOccurs="1" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation>True if user has just read-only privilege</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="domainReadOnly" type="xs:boolean" minOccurs="1" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation>True if user has just read-only privilege in current domain</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="systemObject" type="xs:boolean" minOccurs="1" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation>True if system defined object</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="group" type="xs:boolean" minOccurs="1" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation>True if object is a group</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="valueList" type="NamedEntry" minOccurs="0" maxOccurs="unbounded">
                        <xs:annotation>
                            <xs:documentation>List of literals in case of group</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="references" type="ObjectRefs" minOccurs="0" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation>Object references in case of groups</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="attributes" type="Attributes" minOccurs="1" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation>Platform specific attributes</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>


	<xs:element name="pkiPolicyEntry" type="PkiPolicyEntry" />
	<xs:complexType name="PkiPolicyEntry">
		<xs:annotation>
			<xs:documentation>Pki Policy Object Entry</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="BasePolicyEntry">
				<xs:sequence>
					<xs:element name="pkiCertificateEntry" type="PkiCertificateEntry"
						minOccurs="0" maxOccurs="unbounded" />
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	
	<!-- PKI List Entry -->
	<xs:element name="pkiCertificateEntry" type="PkiCertificateEntry" />
	<xs:complexType name="PkiCertificateEntry">
		<xs:annotation>
			<xs:documentation>Certificate device mapping entry</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="ObjectEntry">
				<xs:all>
				<xs:element name="description" type="xs:string" minOccurs="0" maxOccurs="1" />
					 <xs:element name="status" type="xs:boolean" minOccurs="0" maxOccurs="1" />
					<xs:element name="enrollmentType" minOccurs="1"
						maxOccurs="1">
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:enumeration value="SCEP" />
								<xs:enumeration value="Manual" />
								<xs:enumeration value="SelfSignedCertficate" />
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element name="caCertificateStatus" minOccurs="1"
						maxOccurs="1">
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:enumeration value="Pending" />
								<xs:enumeration value="Available" />
								<xs:enumeration value="NotAvailable"/>
								<xs:enumeration value="InProgress"/>
								<xs:enumeration value="NotApplicable"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element name="identityCertficateStatus" minOccurs="1"
						maxOccurs="1">
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:enumeration value="Pending" />
								<xs:enumeration value="Available" />
								<xs:enumeration value="NotAvailable"/>
								<xs:enumeration value="InProgress"/>
								<xs:enumeration value="NotApplicable"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element name="isOutdated" type="xs:boolean" minOccurs="0" maxOccurs="1" />
				</xs:all>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
    
    

    <xs:complexType name="SLAMonitorPolicyObjectEntry">
        <xs:complexContent>
            <xs:extension base="PolicyObjectEntry">
                <xs:sequence minOccurs="1" maxOccurs="1">
                    <xs:element name="slaId" type="xs:unsignedInt" minOccurs="1" maxOccurs="1" />
                    <xs:element name="securityZoneObject" type="NamedEntry" minOccurs="1" maxOccurs="unbounded" />
                    <xs:element name="monitoredAddress" type="xs:string" minOccurs="1" maxOccurs="1" />
                    <xs:element name="dataSizeInBytes" type="xs:unsignedInt" minOccurs="0" maxOccurs="1" />
                    <xs:element name="thresholdInMilliSeconds" type="xs:unsignedInt" minOccurs="0" maxOccurs="1" />
                    <xs:element name="timeoutInMilliSeconds" type="xs:unsignedInt" minOccurs="0" maxOccurs="1" />
                    <xs:element name="frequencyInSeconds" type="xs:unsignedInt" minOccurs="0" maxOccurs="1" />
                    <xs:element name="toS" type="xs:unsignedInt" minOccurs="0" maxOccurs="1" />
                    <xs:element name="numberOfPackets" type="xs:unsignedInt" minOccurs="0" maxOccurs="1" />
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <!-- NetworkObjectEntry - Network Object -->
    <xs:element name="networkObjectEntry" type="NetworkObjectEntry" />
    <xs:complexType name="NetworkObjectEntry">
        <xs:annotation>
            <xs:documentation>Representation of Network object</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="PolicyObjectEntry">
                <xs:sequence>
                    <xs:element name="addressType" minOccurs="0" maxOccurs="1">
                        <xs:simpleType>
                            <xs:restriction base="xs:string">
                                <xs:enumeration value="Host" />
                                <xs:enumeration value="Network" />
                                <xs:enumeration value="Range" />
                                <xs:enumeration value="FQDN" />
                                <xs:enumeration value="Group" />
                            </xs:restriction>
                        </xs:simpleType>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="AclOrPrefixEntry">

        <xs:sequence>
            <xs:element name="ACLListorPrefix" minOccurs="1" maxOccurs="1" default="ACL List">
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:enumeration value="ACL List" />
                        <xs:enumeration value="Prefix List" />
                    </xs:restriction>
                </xs:simpleType>

            </xs:element>
            <xs:element name="address" type="ObjectEntry" minOccurs="0" maxOccurs="unbounded" />
        </xs:sequence>

    </xs:complexType>
    <xs:complexType name="AclOrPrefixForIpv6Entry">

        <xs:sequence>
            <xs:element name="ACLListorPrefixIpv6" minOccurs="1" maxOccurs="1" default="ACL">
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:enumeration value="ACL" />
                        <xs:enumeration value="Prefix" />
                    </xs:restriction>
                </xs:simpleType>

            </xs:element>
            <xs:element name="address" type="ObjectEntry" minOccurs="1" maxOccurs="1" />
        </xs:sequence>

    </xs:complexType>
    <xs:complexType name="RouteType">
        <xs:sequence>
            <xs:element name="External1" type="xs:boolean" minOccurs="0" maxOccurs="1" />
            <xs:element name="External2" type="xs:boolean" minOccurs="0" maxOccurs="1" />
            <xs:element name="Internal" type="xs:boolean" minOccurs="0" maxOccurs="1" />
            <xs:element name="Local" type="xs:boolean" minOccurs="0" maxOccurs="1" />
            <xs:element name="NSSA-External1" type="xs:boolean" minOccurs="0" maxOccurs="1" />
            <xs:element name="NSSA-External2" type="xs:boolean" minOccurs="0" maxOccurs="1" />
        </xs:sequence>
    </xs:complexType>
    <!-- RouteMapObjectEntry -->
    <xs:element name="routeMapObjectEntry" type="RouteMapObjectEntry" />
    <xs:complexType name="RouteMapObjectEntry">
        <xs:annotation>
            <xs:documentation>Representation of RouteMapObjectEntry
            </xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="NamedEntry">
                <xs:sequence>
                    <xs:element name="routeMapId" type="xs:long" minOccurs="1" maxOccurs="1" />
                    <xs:element name="seqnumber" type="xs:int" minOccurs="1" maxOccurs="1" />
                    <xs:element name="redistribution" type="xs:boolean" minOccurs="1" maxOccurs="1" />
                    <xs:element name="routeSecurityZones" type="NamedEntry" minOccurs="0" maxOccurs="unbounded" />

                    <xs:element name="v4matchaddress" type="AclOrPrefixEntry" minOccurs="0" maxOccurs="1" />
                    <xs:element name="v4matchnexthop" type="AclOrPrefixEntry" minOccurs="0" maxOccurs="1" />
                    <xs:element name="v4matchroutesrc" type="AclOrPrefixEntry" minOccurs="0" maxOccurs="1" />
                    <xs:element name="v6matchaddress" type="AclOrPrefixForIpv6Entry" minOccurs="0" maxOccurs="1" />
                    <xs:element name="v6matchnexthop" type="AclOrPrefixForIpv6Entry" minOccurs="0" maxOccurs="1" />
                    <xs:element name="v6matchroutesrc" type="AclOrPrefixForIpv6Entry" minOccurs="0" maxOccurs="1 " />
                    <xs:element name="matchmetricroute" type="xs:string" minOccurs="0" maxOccurs="1" />
                    <xs:element name="matchTag" type="xs:string" minOccurs="0" maxOccurs="1" />
                    <!-- match route type is shown as set of check box in UI. for each Name a boolean element is created. -->
                    <xs:element name="RouteType" type="RouteType" minOccurs="0" maxOccurs="1" />

                    <xs:element name="Bandwidth" type="xs:long" minOccurs="0" maxOccurs="1" />
                    <xs:element name="EIGRPdelay" type="xs:long" minOccurs="0" maxOccurs="1" />
                    <xs:element name="EIGRPreliability" type="xs:int" minOccurs="0" maxOccurs="1" />
                    <xs:element name="EIGRPeffective" type="xs:int" minOccurs="0" maxOccurs="1" />
                    <xs:element name="EIGRPMTU" type="xs:long" minOccurs="0" maxOccurs="1" />

                    <xs:element name="metrictype" minOccurs="0" maxOccurs="1">
                        <xs:simpleType>
                            <xs:restriction base="xs:string">
                                <xs:enumeration value="Internal" />
                                <xs:enumeration value="Type-1" />
                                <xs:enumeration value="Type-2" />
                            </xs:restriction>
                        </xs:simpleType>
                    </xs:element>
                    <xs:element name="bgpMatchAsPathAccessList" type="ObjectEntry" minOccurs="0" maxOccurs="unbounded" />
                    <xs:element name="bgpMatchCommunity" type="ObjectEntry" minOccurs="0" maxOccurs="unbounded" />
                    <xs:element name="bgpMatchCommunityExact" type="xs:boolean" minOccurs="0" maxOccurs="1" />
                    <xs:element name="bgpMatchPolicyList" type="ObjectEntry" minOccurs="0" maxOccurs="unbounded" />
                    <xs:element name="bgpSetPrependAsPath" type="xs:string" minOccurs="0" maxOccurs="1" />
                    <xs:element name="bgpSetPrependLastAsPath" type="xs:string" minOccurs="0" maxOccurs="1" />
                    <xs:element name="bgpSetConvertRouteTagAsPath" type="xs:boolean" minOccurs="0" maxOccurs="1" />
                    <xs:element name="bgpSetSpecificCommunity" type="xs:string" minOccurs="0" maxOccurs="1" />
                    <xs:element name="bgpSetSpecificCommuInternet" type="xs:boolean" minOccurs="0" maxOccurs="1" />
                    <xs:element name="bgpSetSpecificCommuAdvertise" type="xs:boolean" minOccurs="0"
                        maxOccurs="1" />
                    <xs:element name="bgpSetSpecificCommuExport" type="xs:boolean" minOccurs="0" maxOccurs="1" />
                    <xs:element name="bgpSetExistingCommunity" type="xs:boolean" minOccurs="0" maxOccurs="1" />
                    <xs:element name="bgpSetCommunityNone" type="xs:boolean" minOccurs="0" maxOccurs="1" />
                    <xs:element name="bgpSetAutomaticTag" type="xs:boolean" minOccurs="0" maxOccurs="1" />
                    <xs:element name="bgpSetIpAddressPrefixList" type="ObjectEntry" minOccurs="0" maxOccurs="1" />
                    <xs:element name="bgpSetIpv6AddressPrefixList" type="ObjectEntry" minOccurs="0" maxOccurs="1" />
                    <xs:element name="bgpSetLocalPreference" type="xs:string" minOccurs="0" maxOccurs="1" />
                    <xs:element name="bgpSetWeight" type="xs:string" minOccurs="0" maxOccurs="1" />
                    <xs:element name="bgpSetSpecificIpAdress" type="xs:string" minOccurs="0" maxOccurs="1" />
                    <xs:element name="bgpSetSpecificIpv6Adress" type="xs:string" minOccurs="0" maxOccurs="1" />
                    <xs:element name="BgpUsePeerAdress" type="xs:boolean" minOccurs="0" maxOccurs="1" />
                    <xs:element name="BgpIpv6UsePeerAdress" type="xs:boolean" minOccurs="0" maxOccurs="1" />
                    <!-- for bgpSetOrigin. Has 2 options LocalIGP or Incomplete -->
                    <xs:element name="setOrigin" minOccurs="0" maxOccurs="1">
                        <xs:simpleType>
                            <xs:restriction base="xs:string">
                                <xs:enumeration value="Local IGP" />
                                <xs:enumeration value="Incomplete" />
                            </xs:restriction>
                        </xs:simpleType>

                    </xs:element>
                    <xs:element name="bgpSetNextHop" type="xs:string" minOccurs="0" maxOccurs="1" />
                    <xs:element name="bgpIpv6SetNextHop" type="xs:string" minOccurs="0" maxOccurs="1" />
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <!-- PortObjectEntry - Port Object -->
    <xs:element name="portObjectEntry" type="PortObjectEntry" />
    <xs:complexType name="PortObjectEntry">
        <xs:annotation>
            <xs:documentation>Representation of Port object</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="PolicyObjectEntry">
                <xs:sequence>
                    <xs:element name="protocol" type="xs:int" minOccurs="1" maxOccurs="1" />
                    <xs:element name="icmpCode" type="xs:string" minOccurs="0" maxOccurs="1" />
                    <xs:element name="icmpMsg" type="xs:string" minOccurs="0" maxOccurs="1" />
                    <xs:element name="port" type="xs:string" minOccurs="1" maxOccurs="1" />
                    <xs:element name="homogeneousTCP" type="xs:boolean" minOccurs="1" maxOccurs="1" />
                    <xs:element name="homogeneousUDP" type="xs:boolean" minOccurs="1" maxOccurs="1" />
                    <xs:element name="validForSourcePorts" type="xs:boolean" minOccurs="1" maxOccurs="1" />
                    <xs:element name="validForUserAppURL" type="xs:boolean" minOccurs="1" maxOccurs="1" />
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    
    <xs:element name="encapsulationPortObjectEntry" type="EncapsulationPortObjectEntry" />
    <xs:complexType name="EncapsulationPortObjectEntry">
    	<xs:annotation>
            <xs:documentation>Representation of Encapsulation Port object</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="PortObjectEntry">
                <xs:sequence>
                	<xs:element name="encapsulationType" type="xs:string"  minOccurs="0" maxOccurs="1"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>    	
    </xs:complexType>

	<!-- ZoneObjectEntry - Security Zone Object -->
    <xs:element name="securityZoneObjectEntry" type="SecurityZoneObjectEntry" />
    <xs:complexType name="SecurityZoneObjectEntry">
        <xs:annotation>
            <xs:documentation>Representation of Security Zone Object</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="PolicyObjectEntry">
                <xs:sequence>
                    <xs:element name="interface" type="xs:boolean" minOccurs="1" maxOccurs="1" />
                    <xs:element name="zoneType" type="xs:string" minOccurs="1" maxOccurs="1" />
                    <xs:element name="interfaceGroup" type="xs:boolean" minOccurs="1" maxOccurs="1" />
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="IkeObjectEntry">
    	<xs:annotation>
    		<xs:documentation>Representation of Ikev1 Proposal entry data</xs:documentation>
    	</xs:annotation>
    	<xs:complexContent>
        <xs:extension base="PolicyObjectEntry">
            <xs:sequence>
                <xs:element name="priority" type="xs:int" minOccurs="0" maxOccurs="1" />
                <xs:element name="hash" type="xs:string" minOccurs="1" maxOccurs="1">
                <xs:simpleType>
            		<xs:restriction base="xs:string">
            		<xs:enumeration value="MD5" />
            		<xs:enumeration value="SHA" />
            		</xs:restriction>
                </xs:simpleType>
                </xs:element>
                
                <xs:element name="encryption" type="xs:string" minOccurs="1" maxOccurs="1">
                <xs:simpleType>
                	<xs:restriction base="xs:string">
                    <xs:enumeration value="aes-128" />
                    <xs:enumeration value="aes-192" />
                    <xs:enumeration value="aes-256" />
                    <xs:enumeration value="des" />
                    <xs:enumeration value="3des" />
                    </xs:restriction>
                </xs:simpleType>
                </xs:element>
                <xs:element name="authentication" type="xs:string" minOccurs="1" maxOccurs="1" >
                	<xs:simpleType>
                		<xs:restriction base="xs:string">
                			<xs:enumeration value="Preshared Key" />
                			<xs:enumeration value="Certificate" />
                		</xs:restriction>
        			</xs:simpleType>
            	</xs:element>
                <xs:element name="dhGroup" type="xs:int" minOccurs="1" maxOccurs="1" >
                	<xs:simpleType>
                		<xs:restriction base="xs:int">
                		<xs:enumeration value="1" />
                		<xs:enumeration value="2" />
                		<xs:enumeration value="5" />
                		<xs:enumeration value="7" />
                		<xs:enumeration value="14" />
                		<xs:enumeration value="15" />
                		<xs:enumeration value="16" />
                		<xs:enumeration value="19" />
                		<xs:enumeration value="20" />
                		<xs:enumeration value="21" />
                		<xs:enumeration value="24" />
                		</xs:restriction>
                	</xs:simpleType>
                </xs:element>
                <xs:element name="lifetime" type="xs:long" minOccurs="1" maxOccurs="1" />
            </xs:sequence>
        </xs:extension>
    </xs:complexContent>
    </xs:complexType>
    
    <xs:complexType name="Ike2ObjectEntry">
	<xs:annotation>
		<xs:documentation>Representation of Ikev2 Proposal entry data</xs:documentation>
	</xs:annotation>
	<xs:complexContent>
    <xs:extension base="PolicyObjectEntry">
        <xs:sequence>
            <xs:element name="priority" type="xs:int" minOccurs="0" maxOccurs="1" />
            <xs:element name="encryption" type="xs:string" minOccurs="0" maxOccurs="unbounded" >
            	<xs:simpleType>
            	<xs:restriction base="xs:string">
            	<xs:enumeration value="AES" />
            	<xs:enumeration value="AES-256" />
            	<xs:enumeration value="DES" />
            	<xs:enumeration value="3DES" />
            	<xs:enumeration value="AES-192" />
            	<xs:enumeration value="AES-GCM" />
            	<xs:enumeration value="AES-GCM-256" />
            	<xs:enumeration value="AES-GCM-192" />
            	</xs:restriction>
            	</xs:simpleType>
    		</xs:element>
            <xs:element name="integrity" type="xs:string" minOccurs="0" maxOccurs="unbounded">
            	<xs:simpleType>
            		<xs:restriction base="xs:string">
            		<xs:enumeration value="MD5" />
            		<xs:enumeration value="SHA" />
            		<xs:enumeration value="SHA512" />
            		<xs:enumeration value="SHA256" />
            		<xs:enumeration value="SHA384" />
            		</xs:restriction>
            		</xs:simpleType>
			</xs:element>
            <xs:element name="prf" type="xs:string" minOccurs="0" maxOccurs="unbounded">
            	<xs:simpleType>
            	<xs:restriction base="xs:string">
            	<xs:enumeration value="MD5" />
            	<xs:enumeration value="SHA" />
            	<xs:enumeration value="SHA512" />
            	<xs:enumeration value="SHA256" />
