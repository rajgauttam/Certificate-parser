	<xs:element name="deviceFeatureInfo" type="DeviceFeatureInfo" />
	<xs:complexType name="DeviceFeatureInfo">
		<xs:sequence>
			<xs:element name="deviceUUID" type="xs:string" minOccurs="1"
				maxOccurs="1" />
			<xs:element name="deviceMode" type="DeviceModeType"
				minOccurs="1" maxOccurs="1" default="routed" />
			<xs:element name="deviceModelNo" type="xs:string"
				minOccurs="1" maxOccurs="1" />
			<xs:element name="interfaceCapability" type="InterfaceCapabilityInfo"
				minOccurs="1" maxOccurs="1" />
			<xs:element name="deviceClusterSupport" type="DeviceClusterSupportInfo"
				minOccurs="1" maxOccurs="1" />
			<xs:element name="isClusteredDevice" type="xs:boolean"
				minOccurs="1" maxOccurs="1" />
			<xs:element name="isHADevice" type="xs:boolean" 
				minOccurs="1" maxOccurs="1" />
			<xs:element name="interfacePgUUID" type="xs:string"
				minOccurs="1" maxOccurs="1" />
			<xs:element name="inlineSetPgUUID" type="xs:string"
				minOccurs="1" maxOccurs="1" />
			<xs:element name="deviceFtwCapable" type="xs:boolean" 
				minOccurs="1" maxOccurs="1" />
			<!-- Othe kind of attributes can be added here like NAT etc. -->
		</xs:sequence>
	</xs:complexType>

	<xs:element name="interfaceCapabilityInfo" type="InterfaceCapabilityInfo" />
	<xs:complexType name="InterfaceCapabilityInfo">
		<xs:sequence>
			<xs:element name="deviceMode" type="DeviceModeType"
				minOccurs="1" maxOccurs="1" default="routed" />

			<!-- Max no of interface supported in device -->
			<xs:element name="maxInterfaceCounts" type="xs:unsignedInt"
				minOccurs="1" maxOccurs="1" />

			<!-- Max no of failover interface policy number in device -->
			<xs:element name="maxFailoverInterfaces" type="xs:unsignedInt"
				minOccurs="0" maxOccurs="1" />
				
			<!-- Max no of vlan interface supported in device -->
			<xs:element name="maxNgfwVlanCount" type="xs:unsignedInt"
				minOccurs="1" maxOccurs="1" />

			<!-- Max no of Number of named mgmt Interfaces supported in device -->
			<xs:element name="namedMgmInterfaceCount" type="xs:unsignedInt"
				minOccurs="1" maxOccurs="1" />

			<!-- Can user create sub interface on mgmt interface -->
			<xs:element name="subInterfaceOnMgmtInterface" type="xs:boolean"
				minOccurs="1" maxOccurs="1" />

			<!-- Can user convert default mgmt interface to traffic interface -->
			<xs:element name="defaultMgmToTrafficInterface" type="xs:boolean"
				minOccurs="1" maxOccurs="1" />

			<!-- Can user convert traffic interface to mgmt interface -->
			<xs:element name="trafficToMgmtInterface" type="xs:boolean"
				minOccurs="1" maxOccurs="1" />

			<!-- Can user configure ipv4 -->
			<xs:element name="ipv4ConfigOnInterface" type="xs:boolean"
				minOccurs="1" maxOccurs="1" />

			<!-- Can user create bridge Group -->
			<xs:element name="bridgeGroupInterfaces" type="xs:boolean"
				minOccurs="1" maxOccurs="1" />

			<!-- Can user create pppoe ipv4 config -->
			<xs:element name="pppoeConfigOnInterfaces" type="xs:boolean"
				minOccurs="1" maxOccurs="1" />

			<!-- max no of non-mgmt interafces -->
			<xs:element name="maxNamedNonMgmtInterfaceCount" type="xs:unsignedInt"
				minOccurs="1" maxOccurs="1" />

			<!-- jumboFrame size -->
			<xs:element name="jumboFrame" type="xs:unsignedInt"
				minOccurs="1" maxOccurs="1" />
				
			<!-- Device Model Number -->
			<xs:element name="deviceModelNo" type="xs:string"
				minOccurs="1" maxOccurs="1" />
				
			<!-- Can user create Redundant Interface -->
			<xs:element name="redundantInterfaces" type="xs:boolean"
				minOccurs="1" maxOccurs="1" />
				
			<!-- Can user create Ether Channel Interface -->
			<xs:element name="etherChannelInterfaces" type="xs:boolean"
				minOccurs="1" maxOccurs="1" />
				
			<!-- Is VxLan Feature Support is there or not -->
			<xs:element name="vxLanSupported" type="xs:boolean"
				minOccurs="1" maxOccurs="1" />

			<!-- Othe kind of attributes can be added here -->
		</xs:sequence>
	</xs:complexType>


