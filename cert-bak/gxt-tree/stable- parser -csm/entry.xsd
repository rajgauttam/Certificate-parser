    <!-- CommunityListEntry -->
    <xs:element name="communityListEntry" type="CommunityListEntry" />
    <xs:complexType name="CommunityListEntry">
        <xs:annotation>
            <xs:documentation>Representation of CommunityListEntry
            </xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="NamedEntry">
                <xs:sequence>
                    <xs:element name="communityId" type="xs:long" minOccurs="0" maxOccurs="1" />
                    <xs:element name="seqnumber" type="xs:long" minOccurs="0" maxOccurs="1" />
                    <xs:element name="communitylisttype" type="xs:string" minOccurs="0" maxOccurs="1" />
                    <xs:element name="permit" type="xs:boolean" minOccurs="1" maxOccurs="1" />
                    <xs:element name="standardcommunities" type="xs:string" minOccurs="0" maxOccurs="1" />
